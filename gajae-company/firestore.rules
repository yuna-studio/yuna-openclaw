rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    match /commands/{document} {
      allow read: if true;
      allow write: if false;
    }
    match /meetings/{document} {
      allow read: if true;
      allow write: if false;
    }
    match /{document=**} {
      allow read, write: if false;
    }
  }
}
